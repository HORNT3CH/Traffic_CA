@{
    ViewData["Title"] = "Docklot Print";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>My View</title>
    <link href="/css/StageSheet.css" rel="stylesheet" />
</head>
<body>
    <div class="hide-me">
        @Model IEnumerable<Traffic_CA.GetDockLot>
    </div>
    @if (Model.Count == 0)
    {
        <h1 class="mt-5">No Records to Display</h1>
    }
    else
    {
        <h1 class="mt-5">DockLot Print Sheet</h1>
        <hr />
        <div class="container-fluid">
            <table class="table table-bordered">
                <tr>
                    <th>Location</th>
                    <th>Carrier Name</th>
                    <th>X</th>
                    <th>Trailer Number</th>
                    <th>Status</th>
                    <th>Trailer Comments</th>
                    <th>MBOLNbr</th>
                    <th>Date</th>
                </tr>
                @foreach (var stop in Model)
                {
                    <tr>
                        <td>@stop.Location</td>
                        <td>@stop.CarrierName</td>
                        <td></td>
                        <td>@stop.TrailerNumber</td>
                        <td>@stop.Status</td>
                        <td>@stop.TrailerComments</td>
                        <td>@stop.MBOLNbr</td>
                        <td>@stop.DocklotDate</td>
                    </tr>
                }
            </table>
        </div>
    }
</body>
</html>



